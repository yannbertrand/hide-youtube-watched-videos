{
  "manifest_version": 2,
  "name": "Hide YouTube watched videos",
  "version": "1.0.1",
  "description": "Hide watched videos from your YouTube feed",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/feed/subscriptions"],
      "js": ["youtubeVideosFinder.js", "videosDisplayManager.js", "index.js"]
    }
  ]
}